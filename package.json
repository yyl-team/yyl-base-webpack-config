{
  "version": "0.3.3",
  "main": "output/index.js",
  "types": "output/index.d.ts",
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "yyl-react"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/output",
    "/test"
  ],
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./**",
    "dev": "rollup --config ./rollup.config.js",
    "d": "rollup --config ./rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "w": "rollup --watch --config ./rollup.config.js",
    "prepublishOnly": "npm run prettier && npm test",
    "test": "npm run build && jest ./test/jest/",
    "test:middleware": "npm run build && jest ./test/jest/middleware.test.js",
    "cbw": "cd test/case/base && node ./runner",
    "cmw": "cd test/case/middleware && node ./runner"
  },
  "peerDependencies": {
    "webpack": ">=5.24.0",
    "webpack-cli": ">=4.0.0",
    "webpack-dev-server": ">=3.0.0"
  },
